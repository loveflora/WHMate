{
  "expo": {
    "name": "WHMate",
    "slug": "WHMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "whmate",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.whmate",
      "config": {
        "googleMapsApiKey": "AIzaSyDed2M3tbN3wVTD4kcW2KTtwlgY4lZf-kg"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "이 앱은 당신의 위치를 지도에 표시하기 위해 위치 정보를 사용합니다.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBa9XrimFFISxK0gjq7kIi_jVxEnO05Kyw"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.sarahinaus.whmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c348dbb-1fbd-424e-b5a3-624692c34542"
      }
    }
  }
}
